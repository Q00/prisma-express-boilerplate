enum Sex {
  male female
}

enum Provider {
  kakao
}



model User {
  id          Int         @id
  nickname    String
  birthday    DateTime
  sex         Sex
  userPushId  String
  userAccount UserAccount @unique
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
  deletedAt   DateTime
}

model UserAccount {
  id        Int       @id
  snsId     String
  provider  Provider
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
}

model Nfc {
  id        Int       @id
  value     String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
}

model Routine {
  id        Int       @id
  name      String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
}

model Alarm {
  id        Int       @id
  name      String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
}

model result {
  id        Int       @id
  date      DateTime
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
}

model result_timestamp {
  id           Int      @id
  firstTagging DateTime
  lastTagging  DateTime
}

model user_nfc {
  id          Int       @id
  description String
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  deletedAt   DateTime?
}

model user_nfc_routine {
  id        Int       @id
  daysArray String
  fromTime  DateTime
  endTime   DateTime
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
}

model user_nfc_alaram {
  id        Int       @id
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
}